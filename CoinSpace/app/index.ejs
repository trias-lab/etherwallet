    <script>

        (function(){
            // 底部点击事件
            $('.common-footer .toggle-button').click(function () {
                if ($(this).find('.toggle-box').css('display') === 'none') {
                    $('.toggle-box').hide();
                    $(this).find('.toggle-box').toggle();
                } else {
                    $('.toggle-box').hide();
                }
            })
            // 下拉其他项目导航链接
            $('.toggle-other-link').click(function(){
                $('.select-list-toggle').toggle()
            })
            // 显示钱包信息和用户设置
            $('.toggle-user-wallet').click(function(){
                $('.frame').toggleClass('frame-showWallet')
                $('.left-info').toggleClass('left-info-showWallet')
            })
            // 切换其他钱包
            $('.header-link').find('div.toggle-wallet').click(function(){
                $(this).find('div.pick-wallet-list').toggle();
            })
            $('.header-link-phone').find('div.toggle-wallet').click(function(){
                $(this).find('div.pick-wallet-list').toggle();
            })
        })()

    </script>
